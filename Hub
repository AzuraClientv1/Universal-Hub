local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Universal Hub", "Midnight")
 
-- TABS/SECTIONS
local Bedwars = Window:NewTab("Bedwars")
local Bedwars = Bedwars:NewSection("Bedwars")

local Jailbreak = Window:NewTab("Jailbreak")
local Jailbreak = Jailbreak:NewSection("Jailbreak")

local PetSimX = Window:NewTab("PetSim X")
local PetSimX = PetSimX:NewSection("Pet Simulator X")

local Universal = Window:NewTab("Universal")
local Universal = Universal:NewSection("Universal Scripts")
--END OF TABS/SECTIONS
 
 
 
