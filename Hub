--PET SIMULATOR X TAB
PetSimX:NewButton("AutoCollect", "autocollects orbs and lootbags"function()
  while task.wait() do
for i,v in pairs(game:GetService("Workspace")["__THINGS"].Orbs:GetChildren()) do
    v.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
end
for i,v in pairs(game:GetService("Workspace")["__THINGS"].Lootbags:GetChildren()) do
    v.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
end
end
end)
PetSimX:NewButton("SynapseHub", "Supports PSX, JailBreak,Project Lazarus"function()
  loadstring(game:HttpGet("https://gamingresources.github.io/SynapseHub/PetSimX.lua"))()
end)
PetSimX:NewButton("Project Evo", "Supports PSX, JailBreak,Project Lazarus"function()
  loadstring(game:HttpGet("https://projectevo.xyz/v4/script.lua", true))()
end)
